.getcoupon{
  margin:20rpx;
  flex-direction: column;
}
.coup{
  flex-direction: column;
    background:#fff;
padding:20rpx;
border-radius:15rpx;
margin-bottom:20rpx;
}
.coupons{
  justify-content: space-between;

}
.coupon-one{
  flex-direction: column;
}
.cous{
  flex-direction: row;
  margin:0 auto;
}
.cous text:first-child{
  font-size:34rpx;
  font-weight:bold;
  margin-top:24rpx;
}
.cous text:last-child{
  font-size:60rpx;
  font-weight:bold;
}
.coupon-one>text{
  font-size:26rpx;
  text-align: center;
}
.coupon-two{
  flex-direction: column;
}
.coupon-two>text:last-child{
  font-size:24rpx;
background:#ddd;
text-align:center;
padding:5rpx 10rpx;
margin-top:20rpx;
}
.coupon-three{
  height:52rpx;
font-size:23rpx;
background:#0789ca;
padding:10rpx 20rpx;
color:#fff;
border-radius:5px;
}
.coupon-time{
  text-align:center;
font-size:26rpx;
margin-top:20rpx;
}
