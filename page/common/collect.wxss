page{
  background:#fff;
}
.detail-list{
  flex-direction: column;
  border-top:1px solid #eee;
}
.list-one{
  flex-direction: row;
  padding-bottom:20rpx;
  margin:20rpx;
  border-bottom:1px solid #eee;
}
.list-one>image{
  width:170rpx;
  height:170rpx;
}
.list-two{
  flex-direction: column;
  width:560rpx;
  margin-left:20rpx;
}
.isadd{
  width:45rpx;
  height:45rpx;
}
.list-name{
  text-overflow:ellipsis;
display:-webkit-box;
-webkit-box-orient:vertical;
-webkit-line-clamp:1;
overflow:hidden;
font-size:30rpx;
}
.list-head{
  font-size:26rpx;
color:#999;
height:34rpx;
margin:10rpx 0;
}
.list-price{
  color:#FF840B;
  margin-bottom:10rpx;
}
.list-price>text:first-child{
  font-size:22rpx;
  margin-top:5rpx;
}
.list-price>text:last-child{
  font-size:28rpx
}
.stand{
  justify-content: space-between;
  font-size:24rpx;
}
.stand>image{
  text-align: right;
}
.cancel{
padding:10rpx 20rpx;
border:1px solid #999;
border-radius:10rpx;
font-size:22rpx;
}
.no-data-view{flex-direction:column;align-items:center;padding-top:100rpx}
.no-data-image{width:146rpx;height:160rpx;margin-bottom:50rpx}
.no-data-text{text-align:center;color:#999;line-height:130%;font-size:28rpx}
.no-data-btn{width:176rpx;height:60rpx;border-width:0;margin-top:38rpx;text-align:center;background:#ffe71c;border-radius:6rpx;font-size:28rpx;line-height:60rpx;margin-bottom:48rpx}