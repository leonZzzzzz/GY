.search-msg{flex-direction:column;z-index:9999999;position:fixed;top:0;display:flex;width:750rpx;height:230rpx;background-color:#fff}.search-input{width:625rpx;height:60rpx;position:relative;background:#efefef;margin-left:21rpx;margin-top:7rpx;border-radius:5rpx}.search-image{position:absolute;width:30rpx;height:30rpx;left:20rpx;top:15rpx}.search-text{position:absolute;font-size:26rpx;font-family:PingFangSC-Regular;font-weight:400;color:#000;left:60rpx;display:inline-block;width:450rpx;height:60rpx}.cancel{font-size:24rpx;margin-left:25rpx;line-height:60rpx;margin-top:9rpx}.search-box{justify-content:space-between;display:-webkit-flex;display:flex;flex-direction:row;flex-wrap:wrap;padding:20rpx;margin-top:150rpx;}

.search-data:nth-child(odd){display:flex;flex-direction:column;margin-right:10rpx;margin-bottom:20rpx;width:345rpx;height:495rpx;background:#fff}
.search-data:nth-child(even){display:flex;flex-direction:column;margin-left:10rpx;margin-bottom:20rpx;width:345rpx;height:495rpx;background:#fff}

.search-img{width:345rpx;height:345rpx}.search-img image{width:100%;height:100%}.search-con{flex-direction:column;font-size:24rpx}.con-text{font-weight:bold;color:#333;padding:10rpx;line-height:35rpx;overflow:hidden;text-overflow:ellipsis;display:-webkit-box;word-break:break-all;-webkit-box-orient:vertical;-webkit-line-clamp:2;margin:5rpx;height:60rpx}.con-price{padding:10rpx}.con-price text:nth-child(1){color:#d24749;font-size:36rpx}.con-price text:nth-child(2){margin-top:11rpx;color:#999;text-decoration:line-through;margin-left:10rpx}.normal{font-size:30rpx;color:#333;justify-content:space-around;line-height:500rpx}.weui-loading{margin:0 5px;width:20px;height:20px;display:inline-block;vertical-align:middle;-webkit-animation:weuiLoading 1s steps(12,end) infinite;animation:weuiLoading 1s steps(12,end) infinite;background:transparent url(data:image/svg+xml;base64,) no-repeat;background-size:100%}.weui-loadmore{width:30%;margin:1.5em auto;line-height:1.6em;font-size:14px;text-align:center}.weui-loadmore__tips{display:inline-block;vertical-align:middle}
